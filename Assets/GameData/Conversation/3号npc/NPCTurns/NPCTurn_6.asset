  - textPrompt: "\u65E5\u6599\u5E97"
    textLine: "\u65E5\u6599\u5E97\u4F60\u559C\u6B22\u5417\uFF1F\u6211\u6536\u85CF\u4E86\u4E00\u5BB6\u65E5\u6599\u5E97\u5C31\u5728\u8FD9\u9644\u8FD1\u3002"
    npcResponse: "\u63D0\u5230\u65E5\u6599\u5C31\u662F\u4E09\u6587\u9C7C\u548C\u70E7\u9E1F\u4E86\u5427\uFF1F"
  - textPrompt: "\u706B\u9505\u5E97"
    textLine: "\u6700\u8FD1\u6709\u70B9\u964D\u6E29\uFF0C\u6211\u6536\u85CF\u4E86\u51E0\u5BB6\u706B\u9505\u5E97\u3002"
