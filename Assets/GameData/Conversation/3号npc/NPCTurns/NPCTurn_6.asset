  - textPrompt: "\u4E1C\u5317\u83DC\u5E97"
    textLine: "\u6211\u6700\u8FD1\u8FF7\u4E0A\u4E1C\u5317\u83DC\u4E86\uFF0C\u592A\u597D\u5403\u4E86\uFF01"
    npcResponse: "\u6211\u4E4B\u524D\u5237\u5230\u4ED6\u4EEC\u7684\u83DC\u91CF\u9887\u4E3A\u60CA\u4EBA\u3002"
