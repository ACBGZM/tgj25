    npcResponse: "\u5514\uFF0C\u5927\u6982\u6237\u5916\u7684\u6E38\u73A9\u9009\u62E9\u6BD4\u8F83\u591A\u3002"
  - textPrompt: "\u65E0\u6240\u8C13"
    textLine: "\u6211\u90FD\u8FD8\u597D\u5566\uFF0C\u770B\u5BF9\u65B9\u60F3\u505A\u4EC0\u4E48\uFF01"
    npcResponse: "\u5514\uFF0C\u628A\u9009\u62E9\u6743\u4EA4\u7ED9\u5BF9\u65B9\u5417\u3002"
